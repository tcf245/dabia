{
  "buildCommand": "pip install -r requirements.txt && alembic upgrade head",
  "builds": [
    {
      "src": "dabia/main.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": "data/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dabia/main.py"
    }
  ]
}
